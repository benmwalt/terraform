provider "aws" {
  access_key = "${var.access_key}"
  secret_key = "${var.secret_key}"
  region     = "${var.region}"
}
	      
resource "aws_instance" "master" {
  ami           = "ami-13be557e"
  instance_type = "t2.micro"
}

resource "aws_instance" "slave1" {
  ami           = "ami-13be557e"
  instance_type = "t2.micro"
}

resource "aws_instance" "slave2" {
  ami           = "ami-13be557e"
  instance_type = "t2.micro"
}

resource "aws_eip" "ip" {
  instance = "${aws_instance.master.id}"
}
